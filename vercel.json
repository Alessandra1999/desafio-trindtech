{
    "version": 2,
    "builds": [
        {
            "src": "./backend/api/package.json",
            "use": "@vercel/node"
        },
        {
            "src": "./frontend/package.json",
            "use": "@vercel/react"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/backend/api/$1"
        }
    ]
}